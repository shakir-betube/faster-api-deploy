    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "concurrently": "^3.6.1",
    "nodemon": "^1.19.4",
    "ntypescript": "^1.201706190042.1",
